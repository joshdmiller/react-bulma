{
  "name": "react-bulma",
  "version": "1.0.0",
  "description": "A thin wrapper of React components around bulma.io",
  "main": "./lib",
  "scripts": {
    "build": "babel -d lib/ src/",
    "test": "tape -r babel-register 'src/**/*spec.js' | faucet"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshdmiller/react-bulma.git"
  },
  "keywords": [
    "react",
    "bulma",
    "css",
    "flexbox",
    "responsive",
    "framework"
  ],
  "author": "Josh David Miller <josh@joshdavidmiller.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joshdmiller/react-bulma/issues"
  },
  "homepage": "https://github.com/joshdmiller/react-bulma#readme",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "faucet": "0.0.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.3"
  }
}
